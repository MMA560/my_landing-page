{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node",
        "config": {
          "buildCommand": "npm run build"
        }
      }
    ],
    "functions": {
      "api/*": {
        "runtime": "nodejs16.x"
      }
    },
    "env": {
      "NODE_ENV": "production"
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  